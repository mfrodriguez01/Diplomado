<p id="notice"><%= notice %></p>

<h1>Listing Programmings</h1>

<table>
  <thead>
    <tr>
      <th>Airplane</th>
      <th>Track</th>
      <th>Pilot</th>
      <th>Date</th>
      <th>Hour</th>
      <th>Origin</th>
      <th>Destination</th>
      <th>Duration</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programmings.each do |programming| %>
      <tr>
        <td><%= programming.airplane %></td>
        <td><%= programming.track %></td>
        <td><%= programming.pilot %></td>
        <td><%= programming.date %></td>
        <td><%= programming.hour %></td>
        <td><%= programming.origin %></td>
        <td><%= programming.destination %></td>
        <td><%= programming.duration %></td>
        <td><%= link_to 'Show', programming_path(programming.id) %></td>
        <td><%= link_to 'Edit', edit_programming_path(programming.id) %></td>
        <td><%= link_to 'Destroy', programming, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Programming', new_programming_path %>
